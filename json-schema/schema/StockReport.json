{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "StockReport",
  "type": "object",
  "properties": {
    "ticker": {
      "type": "string",
      "description": "ID of the stock in stock exchange"
    },
    "score": {
      "type": "string"
    },
    "one_month_price_change": {
      "type": "string"
    },
    "long_term_total_return": {
      "type": "string"
    }
  },
  "required": [
    "ticker",
    "score",
    "one_month_price_change",
    "long_term_total_return"
  ]
}
